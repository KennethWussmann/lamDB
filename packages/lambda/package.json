{
  "name": "@lamdb/lambda",
  "version": "1.0.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "prepack": "npm run build",
    "dev": "npm run build --watch",
    "build": "tsc",
    "test": "jest --maxWorkers=50% --passWithNoTests",
    "clean": "find test src -name '*.d.ts' -or -name '*.js' -or -name '*.map' | xargs rm",
    "release": "npm version $PACKAGE_VERSION --allow-same-version --no-git-tag-version && npm publish"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.188.0",
    "@lamdb/core": "*",
    "@prisma/engine-core": "^4.4.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.107"
  }
}
