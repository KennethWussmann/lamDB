{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "./build/**",
        "src/**/*.js",
        "src/**/*.d.ts",
        "src/**/*.map",
        "test/**/*.js",
        "test/**/*.d.ts",
        "test/**/*.map",
        "lib/**/*.js",
        "lib/**/*.d.ts",
        "lib/**/*.map",
        "bin/**/*.js",
        "bin/**/*.d.ts",
        "bin/**/*.map",
        "dist/**"
      ]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "deploy": {
      "cache": false
    }
  }
}
